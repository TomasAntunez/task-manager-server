{
  "compilerOptions": {
    "target": "ES2021",
    "module": "NodeNext",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "./dist",
  }
}
